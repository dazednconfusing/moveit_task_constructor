<?xml version="1.0"?>
<launch>
  <arg name="debug" default="false" />
  <arg unless="$(arg debug)" name="launch_prefix" value="" />
  <arg     if="$(arg debug)" name="launch_prefix" value="gdb -ex run --args" />
  <!-- Run MTC pick and place -->
  <node name="mtc_tutorial"
   pkg="moveit_task_constructor_demo" 
   type="pick_place_demo" 
   output="screen"
    launch-prefix="$(arg launch_prefix)">
    <param name="execute" value="true" />
    <rosparam command="load" file="$(find moveit_task_constructor_demo)/config/loco_config.yaml" />
  </node>
</launch>
